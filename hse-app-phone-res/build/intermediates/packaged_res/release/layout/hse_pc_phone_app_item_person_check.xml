<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:paddingBottom="@dimen/hse_pc_phone_app_item_person_check_parent_paddingbottom"
        android:paddingLeft="@dimen/hse_pc_phone_app_item_person_check_parent_paddingleft"
        android:paddingTop="@dimen/hse_pc_phone_app_item_person_check_parent_paddingtop" >

        <TextView
            android:id="@+id/hse_pc_phone_app_item_person_check_tv_type"
            android:layout_width="@dimen/hse_pc_phone_app_item_person_check_tv_type_width"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/hse_pc_phone_app_item_person_check_tv_type_marginright"
            android:text="入场证号"
            android:textColor="@color/hd_hse_common_describetext_gray"
            android:textSize="@dimen/hd_common_textsize" />

        <TextView
            android:id="@+id/hse_pc_phone_app_item_person_check_tv_information"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="125466225"
            android:textColor="@color/hd_hse_common_component_phone_fontnum"
            android:textSize="@dimen/hd_common_textsize" />
    </LinearLayout>

    

</LinearLayout>